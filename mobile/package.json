    "eas:build:ios": "eas build --platform ios",
    "eas:build:preview": "eas build --profile preview",
    "eas:submit": "eas submit",
    "eas:update": "eas update",
    "publish": "expo publish",
    "clean": "yarn clean:expo && yarn clean:node",
    "clean:expo": "expo r -c",
    "clean:node": "rm -rf node_modules && yarn install",
